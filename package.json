{
  "name": "hangman",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "startexpress": "node -r dotenv/config bin/www",
    "production": "docker-compose -f docker-compose.production.yml up",
    "productionbuild": "docker-compose -f docker-compose.production.yml build",
    "test": "node node_modules/mocha/bin/mocha",
    "createdockercontainer": "docker run --name hangmandb -p 27017:27017 -v mongoman:/data/db mongo",
    "startdb": "docker start hangmandb",
    "stopdb": "docker stop hangmandb",
    "resetdb": "mongo 127.0.0.1:27017/hangmanDB services/repository/create-db-script.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "mongodb": "latest",
    "morgan": "~1.9.1",
    "dotenv": "latest",
    "crypto-js": "latest",
    "jsonwebtoken": "latest"
  },
  "devDependencies": {
    "@types/mongodb": "latest",
    "@types/cookie-parser": "latest",
    "@types/body-parser": "latest",
    "@types/debug": "latest",
    "@types/express": "latest",
    "@types/hbs": "latest",
    "@types/http-errors": "latest",
    "@types/mocha": "^8.2.1",
    "@types/morgan": "latest",
    "@types/node": "^14.14.31",
    "chai": "latest",
    "mocha": "latest",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "latest"
  }
}
